



<h1 style="text-align: center; margin-top: 60px;">Seattle Community Gardens</h1>


<div class="garden-index-page-inner">
<div class="container">
  <div class="row">
  <div class="col-md-2"></div>
    <div class="col-md-8">
    <% @gardens.each do |garden| %>
    

    <div class="card">
      <div class="card-body">
        <h5 class="card-title"><%= link_to garden.name, garden_path(garden) %></h5>
        <p class="card-text"><small class="text-muted"><%= garden.address %></small></p>
        <p class="card-text"><%=garden.description %></p>
        
      </div>
      <img class="card-img-bottom" src="<%= garden.img_url %>" alt="Card image cap">
    </div>
    <% end %>
    </div>
    <div class="col-md-2"></div>
  </div>
</div>
</div>